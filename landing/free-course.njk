  </div>
</section>

<!-- CTA Section -->
<section class="cta section section--sage">
  <div class="container">
    <div class="cta__inner">
      <p class="eyebrow" data-editable="text" data-prop="cta_eyebrow">{{ cta_eyebrow }}</p>
      <h2 class="cta__title" data-editable="text" data-prop="cta_title">{{ cta_title }}</h2>
      <p class="subtitle cta__subtitle" data-editable="text" data-prop="cta_subtitle">{{ cta_subtitle }}</p>
      <a href="#signup" class="btn btn--primary btn--large" data-editable="text" data-prop="cta_button">{{ cta_button }}</a>
    </div>
  </div>
</section>

<script>
// Form submission with webhook
document.getElementById('course-form').addEventListener('submit', function(e) {
  e.preventDefault();
  const form = this;
  const formData = new FormData(form);
  const redirectUrl = form.dataset.redirect;

  // Submit to webhook (update action URL in CloudCannon)
  if (form.action && form.action !== '#' && form.action !== '') {
    fetch(form.action, {
      method: 'POST',
      body: formData,
      mode: 'no-cors'
    }).then(function() {
      if (redirectUrl) {
        window.location.href = redirectUrl;
      }
    }).catch(function(error) {
      console.error('Error:', error);
      if (redirectUrl) {
        window.location.href = redirectUrl;
      }
    });
  } else {
    // Demo mode - just redirect
    if (redirectUrl) {
      window.location.href = redirectUrl;
    }
  }
});
</script>
