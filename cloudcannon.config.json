      "url": "/lp/[slug]/",
      "disable_url": false,
      "description": "Lead magnet and opt-in landing pages with forms",
      "_enabled_editors": [
        "visual",
        "data"
      ],
      "add_options": [
        {
          "name": "New Lead Magnet Page",
          "schema": "landing_page"
        }
      ],
      "disable_add": false,
      "disable_add_folder": false,
      "disable_file_actions": false,
      "schemas": {
        "landing_page": {
          "path": "schemas/landing-page.md",
          "reorder_inputs": true,
          "hide_extra_inputs": false,
          "remove_empty_inputs": false,
          "remove_extra_inputs": false,
          "name": "Lead Magnet Page",
          "_inputs": {
            "$.layout": {
              "cascade": true
            }
          }
        }
      }
    },
    "blog": {
      "path": "blog",
      "name": "Blog Posts",
      "icon": "article",
