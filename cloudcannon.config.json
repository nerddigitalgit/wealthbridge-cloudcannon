  "collections_config": {
    "pages": {
      "path": "",
      "url": "/[slug]/",
      "output": true,
      "icon": "wysiwyg",
      "name": "Pages",
      "description": "Main website pages",
      "_enabled_editors": ["visual", "content", "data"],
      "schemas": {
        "default": {
          "path": "_schemas/page.json"
        }
      }
    },
    "landing_pages": {
      "path": "landing",
      "url": "/landing/[slug]/",
      "output": true,
      "icon": "rocket_launch",
      "name": "Landing Pages",
      "description": "Marketing and conversion landing pages",
      "_enabled_editors": ["visual", "content", "data"]
    },
    "data": {
      "path": "_data",
      "output": false,
      "icon": "settings",
      "name": "Site Data",
      "description": "Global site configuration and content"
    }
  },
  "data_config": {
    "site": {
      "path": "_data/site.json"
